<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.doctor.persistance.AntecedentChir" table="antecedentchir"
		>
		<id name="idantecedentChir" type="java.lang.Integer">
			<column name="idantecedentChir" />
			<generator class="increment" />
		</id>
		<property name="antChirugical" type="string">
			<column name="antChirugical" length="255" />
		</property>

	</class>
	</hibernate-mapping>