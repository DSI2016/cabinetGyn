<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.doctor.persistance.Diagnostique" table="diagnostique"
		>
		<id name="iddiagnostique" type="java.lang.Integer">
			<column name="iddiagnostique" />
			<generator class="increment" />
		</id>
		


		<property name="diagnostique" type="string">
			<column name="diagnostique" />
		</property>

	</class>
</hibernate-mapping>
