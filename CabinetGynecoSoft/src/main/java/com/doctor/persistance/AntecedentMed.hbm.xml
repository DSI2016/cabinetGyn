<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.doctor.persistance.AntecedentMed" table="antecedentmed"
		>
		<id name="idantecedentMed" type="java.lang.Integer">
			<column name="idantecedentMed" />
			<generator class="increment" />
		</id>
		<property name="antMedical" type="string">
			<column name="antMedical" length="255" />
		</property>

	</class>
	</hibernate-mapping>