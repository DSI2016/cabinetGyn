<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.doctor.persistance.ExamenComplementaire" table="examencomplementaire"
		>
		<id name="idexamenComplementaire" type="java.lang.Integer">
			<column name="idexamenComplementaire" />
			<generator class="increment" />
		</id>
		
		<property name="libExamenComplementaire" type="string">
			<column name="libexamenComplementaire" length="45" />
		</property>

	</class>
	</hibernate-mapping>